                "# https://www.youtube.com/watch?v=487mr-e_Z74\r\n",
                "\r\n",
                "number = 358\r\n",
                "\r\n",
                "rem = rev = 0\r\n",
                "while number >= 1:\r\n",
                "    rem = number % 10\r\n",
                "    rev = rev * 10 + rem\r\n",
                "    number = number // 10\r\n",
                "    print(\"rem:\",rem,\", rev:\",rev,\", number:\",number)\r\n",
                "\r\n",
                "print(rev)"
            ],
